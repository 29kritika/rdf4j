SELECT * where {

       ?review bsbm:price "5325.23"^^<http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/USD> .
       ?review2 ?pred <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromVendor1/Offer1841/> .

       ?review bsbm:product ?product.
       ?review2 bsbm:product ?product.

       ?offer bsbm:product ?product.
       ?offer2 bsbm:product ?product.

       ?offer rdf:type bsbm:Offer.
       ?offer2 rdf:type bsbm:Offer.

       filter(?offer != ?offer2)
}

